{
  "name": "bch-cli-wallet",
  "description": "A command line wallet for Bitcoin Cash, built with BITBOX",
  "version": "1.3.1",
  "author": "Chris Troutner @christroutner",
  "bin": {
    "bch-cli-wallet": "./bin/run"
  },
  "bugs": "https://github.com/Bitcoin-com/bch-cli-wallet",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2.1.3",
    "bitbox-sdk": "^8.4.0",
    "cli-table": "^0.3.1",
    "orbit-db": "^0.21.0",
    "prettier": "^1.15.3",
    "qrcode-terminal": "^0.12.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.5",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^6.0.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "globby": "^9.2.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.3",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.15",
    "sinon": "^7.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/wallets"
  ],
  "homepage": "https://github.com/Bitcoin-com/bch-cli-wallet",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "bch-cli-wallet",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Bitcoin-com/bch-cli-wallet",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "TEST=unit nyc --reporter=text mocha  --timeout 15000",
    "test:integration": "TEST=integration nyc --reporter=text mocha  --timeout 15000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:report": "nyc --reporter=html mocha",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release"
  }
}
